    "report with abstract ": {
        "prefix": "report",
        "body": [
          "\\documentclass[]{article}",
          "\\usepackage{amsmath}",
          "\\usepackage{amsfonts} ",
          "\\usepackage[english]{babel}",
          "\\usepackage{amsthm}",
          "\\usepackage{mathtools}",
          "\\usepackage{subcaption}",
